    "qs": "^6.11.2",
    "rc-slider": "^10.5.0",
    "react": "^18",
    "react-dom": "^18",
    "react-player": "^2.15.1",
    "styled-components": "^6.1.0"
  },
  "devDependencies": {
    "@strapi/strapi": "^4.22.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.11.0",
    "@types/qrcode": "^1.5.5",
    "@types/qs": "^6.9.11",
